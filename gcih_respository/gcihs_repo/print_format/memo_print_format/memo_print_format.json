 "html": "<div>\n    <head>\n        <!-- Font Preloading -->\n        <link rel=\"preconnect\" href=\"https://fonts.googleapis.com\">\n        <link rel=\"preconnect\" href=\"https://fonts.gstatic.com\" crossorigin>\n        <link href=\"https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900&display=swap\" rel=\"stylesheet\">\n        <link href=\"https://fonts.googleapis.com/css2?family=Droid+Serif&display=swap\" rel=\"stylesheet\">\n        <style>\n            body, td, p {\n                font-family: 'Droid Serif', serif;\n            }\n            td {\n                font-size: 14px;\n            }\n            p {\n                line-height: 1.8;\n                margin-bottom: 20px;\n            }\n        </style>\n    </head>\n\n    {{ letter_head }}\n    <table style=\"width: 100%; margin-top: 20px;\">\n        <tr>\n            <td style=\"width: 15%; font-weight: bold;\">FROM:</td>\n            <td style=\"width: 85%;\">{{ doc.name_to }}</td>\n        </tr>\n        <tr>\n            <td style=\"font-weight: bold;\">TO:</td>\n            <td style=\"font-family: 'Poppins', sans-serif\" >{{ doc.name_from }}</td>\n        </tr>\n        <tr>\n            <td style=\"width: 15%; font-weight: bold;\">CC:</td>\n            <td style=\"width: 85%;\">{{ doc.name_to }}</td>\n        </tr>\n        <tr>\n            <td style=\"width: 15%; font-weight: bold;\">REF:</td>\n            <td style=\"width: 85%;\">{{ doc.name_to }}</td>\n        </tr>\n        <tr>\n            <td style=\"font-weight: bold;\">DATE:</td>\n            <td>{{ frappe.utils.formatdate(doc.name_date) }}</td>\n        </tr>\n        <tr>\n            <td style=\"font-weight: bold;\">SUBJECT:</td>\n            <td>{{ doc.name_subject }}</td>\n        </tr>\n    </table>\n    \n    <!-- Add horizontal line -->\n    <hr style=\"border: none; border-top: 2px solid black; margin: 10px 0;\">\n   <div id=first_par>\n        {% for paragraph in doc.content.split('\\n\\n') %}\n            <p>{{ paragraph | safe }}</p>\n        {% endfor %}\n        \n    </div> \n\n</div>",
